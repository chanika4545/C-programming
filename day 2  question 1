#include <stdio.h>
#include <math.h>

int main() {
    float radius, area, circumference;

    printf("Enter radius: ");
    scanf("%f", &radius);

    area = M_PI * radius * radius;
    circumference = 2 * M_PI * radius;

    printf("Area = %.2f, Circumference = %.2f\n", area, circumference);

    return 0;
}
